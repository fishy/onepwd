  <!-- Bio preference -->
  <string name="pref_title_bio_autoload">Load MasterKey on start-up.</string>
  <string name="pref_desc_bio_autoload">
    Try to auto load stored MasterKey on app start-up.
  </string>
  <string name="pref_title_bio_invalidate">
    Invalidate by Biometric Enrollment
  </string>
  <string name="pref_desc_bio_invalidate">
    "Invalidate the stored MasterKey when new biometric (fingerprint) is"
    "enrolled on this device."
    "\nNote that this option does not affect already stored MasterKey."
  </string>

  <!-- Prefill preference -->
  <string name="pref_title_usage">From current app</string>
  <string name="pref_desc_usage_yes">
    Try to prefill the sitekey from the package name of the last running app.
    This is just a best guess, please use with caution.
  </string>
  <string name="pref_desc_usage_no">
    Only prefill the sitekey when sharing URL to OneKey.
  </string>
  <string name="pref_title_permission_usage">
    Grant OneKey permission
  </string>
  <string name="pref_desc_permission_usage">
    Click here to go to Usage Access System Settings.
    OneKey needs this permission in order to prefill sitekey from current app.
  </string>
  <string name="pref_desc_prefill_share">
    "ProTip: You can always just share the URL from browser to OneKey to"
    "get sitekey prefilled from the URL."
  </string>

